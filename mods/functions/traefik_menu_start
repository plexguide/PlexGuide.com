[5] DNS Delay Check     : [$delaycheck] Seconds
------------------------------------------------------------------------------
$minidisplay
------------------------------------------------------------------------------
$alphadisplay
$def
------------------------------------------------------------------------------
[Z] Exit Interface
"
common_number

if [[ "$typed" -ge "$tnumber" ]]; then
common_message "â›”ï¸  Typed a Selection that does not exist | [PRESS ENTER]"
common_confirm && traefik_menu && exit; fi

case $typed in
    1 ) traefik_menu_start && exit ;;
    2 ) traefik_providers  && traefik_menu_start && exit ;;
    3 ) traefik_menu_start && exit ;;
    4 ) traefik_menu_start && exit ;;
    5 ) traefik_menu_start && exit ;;
    6 ) bash /pg/stage/pgcloner/pgpress.sh && start_menu && exit ;;
    z ) start_menu && exit ;;
    Z ) start_menu && exit ;;
    * ) traefik_menu_start && exit ;;
esac





}
